{
  "schemaVersion": 1,
  "id": "mcde",
  "version": "${version}",

  "name": "MC Dungeons: Enchanting",
  "description": "Brings the Enchantment system from Minecraft Dungeons to Minecraft",
  "authors": [
    "BackupCup",
    "Iamnotagenius"
  ],
  "contact": {
    "curseforge": "https://www.curseforge.com/minecraft/mc-mods/mc-dungeons-enchanting",
    "modrinth": "https://www.modrinth.com/mod/mc-dungeons-enchanting",
    "sources": "https://www.github.com/EndLone/MCDE",
    "issues": "https://www.github.com/EndLone/MCDE/issues"
  },

  "license": "Timefall Development License 1.1",
  "icon": "assets/mcde/icon.png",

  "environment": "*",
  "entrypoints": {
    "mcde:init": [
      "net.backupcup.mcde.MCDEnchantments"
    ],
    "main": [
      "net.backupcup.mcde.MCDEnchantments"
    ],
    "client": [
      "net.backupcup.mcde.MCDEClient"
    ]
  },
  "mixins": [
    "mcde.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": ">=1.20 <=1.20.1",
    "java": ">=17"
  },
  "suggests": {
    "mcdw": ">=5.0.12-1.19",
    "mcda": ">=2.4.4",
    "mcdar": ">=1.5.3-1.19"
  },
  "custom": {
    "mc-publish": {
      "modrinth": "iwkcspV8",
      "curseforge": 886511
    }
  }
}
