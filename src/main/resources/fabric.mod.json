{
  "schemaVersion": 1,
  "id": "mcde",
  "version": "${version}",

  "name": "Minecraft Dungeons: Enchantments",
  "description": "Brings the Enchantment system from Minecraft Dungeons to Minecraft",
  "authors": [
    "BackupCup",
    "Iamnotagenius"
  ],
  "contact": {
    "curseforge": "https://legacy.curseforge.com/minecraft/mc-mods/mc-dungeons-enchantments",
    "modrinth": "https://modrinth.com/mod/mc-dungeons-enchantments",
    "sources": "https://github.com/EndLone/MCDE"
  },

  "license": "Timefall Development License 1.1",
  "icon": "assets/mcde/icon.png",

  "environment": "*",
  "entrypoints": {
    "mcde:init": [
      "net.backupcup.mcde.MCDEnchantments"
    ],
    "main": [
      "net.backupcup.mcde.MCDEnchantments"
    ],
    "client": [
      "net.backupcup.mcde.MCDEClient"
    ]
  },
  "mixins": [
    "mcde.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "cloth-config": ">=8.3.103"
  },
  "suggests": {
    "mcdw": ">=5.0.12-1.19",
    "mcda": ">=2.4.4",
  },
  "custom": {
    "mc-publish": {
      "modrinth": "iwkcspV8",
      "curseforge": 886511
    }
  }
}
